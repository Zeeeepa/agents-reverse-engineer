{
  "name": "agents-reverse-engineer",
  "version": "1.1.3",
  "description": "CLI tool for reverse-engineering codebase documentation for AI agents",
  "type": "module",
  "bin": {
    "agents-reverse-engineer": "dist/cli/index.js",
    "are": "dist/cli/index.js"
  },
  "scripts": {
